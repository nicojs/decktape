        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "protocol": "inspector",
            "program": "${workspaceRoot}/decktape.js",
            "args": [
                "http://remarkjs.com/", "test.pdf"
            ]
        }
    ]
}