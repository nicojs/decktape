    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Current file",
            "program": "${file}",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "args": ["3001"]
